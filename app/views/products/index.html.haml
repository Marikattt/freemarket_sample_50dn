        -# カテゴリー毎
        -# レディース
        .bodyMain_pickup_cotegory.clearfix
          .bodyMain_pickup_cotegory_box
            .bodyMain_pickup_cotegory_box--title
              =link_to "レディース新着アイテム",products_path,class:"category-title__link"
            .bodyMain_pickup_cotegory_box_items.clearfix
              - @womens.each do |women|
                .itemBox
                  =link_to product_path(women),  class:"itemBox_a" do
                    = image_tag("#{women.images.first.image}", width: '213', alt: "product_image",class:"itemBox--image")
                    .itemBox_ditails
                      .itemBox_ditails--name
                        ="#{women.name}"
                        .hide
                      .itemBox_ditails_num
                        .itemBox_ditails_num--price
                          ="¥#{women.price.to_s(:delimited)}"
                        -# likeなければ消す if文？？
                        .itemBox_ditails_num--like
                          %i.fa.fa-heart
                        -# ライク数挿入
                      .itemBox_ditails--tax
                        (税込)
            .viewAll
              =link_to "すべての商品を見る", products_path

