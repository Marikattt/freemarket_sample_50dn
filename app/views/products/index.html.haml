  .main
    -# バナー
    .topBanner
      .fas.fa-chevron-left.topBanner--left
      .fas.fa-chevron-right.topBanner--right
      .topBanner_image
        =image_tag "//www-mercari-jp.akamaized.net/assets/img/featured/camp/index/index_keyvisual.png?376951604" ,class: "topBanner--image"
      

    -# 商品
    .bodyMain
